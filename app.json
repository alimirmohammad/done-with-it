{
  "expo": {
    "name": "DoneWithIt",
    "slug": "DoneWithIt",
    "description": "A marketplace for selling the stuff that you don't need anymore.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./app/assets/logo-red.png",
          "color": "#ffffff"
        }
      ]
    ],
    "notification": {
      "iosDisplayInForeground": true
    }
  }
}
